{
  "name": "Chrome Keyboard Visualizer",
  "version": "1.0",
  "description": "An Audio Keyboard Visualizer for Razer merch.",
  "permissions": ["storage", "activeTab", "tabCapture"],
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/icons/keyboard-16.png",
      "24": "assets/icons/keyboard-24.png",
      "32": "assets/icons/keyboard-32.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icons/keyboard-16.png",
    "48": "assets/icons/keyboard-32.png",
    "128": "assets/icons/keyboard-128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
